{
    "name": "kerasai/robo-config",
    "description": "Configuration handling for Robo PHP task runner.",
    "type": "library",
    "authors": [
        {
            "name": "Will Long",
            "email": "will@kerasai.com"
        }
    ],
    "require": {
        "consolidation/robo": "~1 || ~2 || ~3 || ~4"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Kerasai\\Robo\\Config\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kerasai\\Robo\\Config\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    }
}
